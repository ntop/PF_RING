include="@ACCOLADE_INC@ @INVEATECH_INC@ @EXABLAZE_INC@"
libs="@SYSLIBS@ @NT_LIB@ @ACCOLADE_LIB@ @MYRICOM_LIB@ @INVEATECH_LIB@ @EXABLAZE_LIB@ @REDIRECTOR_LIB@"


usage () {
        cat <<EOF
Usage: $0 [OPTIONS]
Options:
        --include        [$include]
        --libs           [$libs]
EOF
        exit 1
}

if test $# -le 0; then usage; fi

while test $# -gt 0; do
        case $1 in
        --include) echo "$include" ;;
        --libs)    echo "$libs" ;;
        *)         usage ;;
        esac

        shift
done

exit 0
